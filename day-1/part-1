#include <stdio.h>
#include <stdlib.h>
#include <string.h>


int main()
{
    FILE *my_file = fopen("input.txt", "r");

    char *str_depth = malloc(sizeof(char) * 5);
    int depth = 1000000, last_depth, counter = 0;
    char temp;

    do{
        last_depth = depth;
        //getting depth
        fscanf(my_file, "%s", str_depth);
        depth = atoi(str_depth);

        if(depth > last_depth){
            ++counter;
        }
        }while((temp = getc(my_file)) != EOF);

    printf("%d", counter);

    fclose(my_file);
    return 0;
    }
